<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Title</th>
      <th>Course</th>
      <th>Serves</th>
      <th>Cooktime</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% recipes.forEach(function(r){ %>
    <tr>
      <td><%= r.title %></td>
      <td><%= r.course %></td>
      <td><%= r.serves %></td>
      <td><%= r.cookTime %></td>
    </tr>
    <% }); %>
  </tbody>
</table>

<%- include('../partials/footer') %>
