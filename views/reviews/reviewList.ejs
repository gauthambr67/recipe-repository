
<%- include('../partials/header') %>

<table id="list">
  <div>
   
    <% reviews.forEach(function(r){ %>
<h2>
  <%= r.content %> 
</h2>
<h2>
  <%= r.recipe %> 
</h2>
<form
          id="add-reviews"
          action="/reviews/recipes/<%= r._id %>/reviews/new?_method=PUT"
          method="POST"
        >
          <input type="submit" value="Edit Review" />
        </form>

<% }); %>
  </div>
  </tbody>

</table>



<%- include('../partials/footer') %>

